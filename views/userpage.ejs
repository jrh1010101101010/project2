<% if (locals.workout[0]){%>

<h2 class="title"><%= locals.workout[0].username %>'s workouts</h2>
<% }%> 
<main class="user-main">
    <% for (ele of locals.workout) { %>
        <div class="user-ui">
            <p><a href="/workout/<%= ele.id %>"><%= ele.title%></a></p>

            <p><%= ele.description%></p>
        </div>
        

    <% }%>

</main>

